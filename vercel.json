{
  "version": 2,
  "projects": [
    {
      "name": "frontend",
      "source": "./frontend"
    },
    {
      "name": "backend",
      "source": "./backend"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ]
}
